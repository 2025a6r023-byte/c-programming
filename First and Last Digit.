#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int last_digit = n % 10;
    int first_digit = n;
    while(first_digit >= 10){
        first_digit /= 10;
    }
    printf("%d %d",first_digit,last_digit);
    return 0;
    }
