#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num,first,last,temp,count=1;
    scanf("%d",&num);
    
    temp = num;
    last = num % 10;
    
    while(temp >= 10){
        temp = temp/10;
        count = count*10;
    }
    first = temp;
    
    num = num % count;
    num = num/10;
    
    int swapped = last*count + num*10 + first;
    printf("%d",swapped);
    return 0;
}
    

    
