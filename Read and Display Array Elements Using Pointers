#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {
    int N;
    scanf("%d", &N);

    int arr[100];
    int *p = arr;  
 
    for (int i = 0; i < N; i++) {
        scanf("%d", (p + i));
    }

    for (int i = 0; i < N; i++) {
        printf("%d", *(p + i));
        if (i < N - 1)
            printf(" ");
    }

    return 0;
}
