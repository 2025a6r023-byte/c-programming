#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char ch;
    scanf("%c",&ch);
    if(ch=='a'||ch=='A'||ch=='e'||ch=='E'||ch=='i'||ch=='I'||ch=='o'||ch=='O'||ch=='u'||ch=='U'){
      printf("Vowel");
        }
     else if (isalpha(ch)){
      printf("Consonant");
   } 
    else{
        printf("Invalid Input");
   } 
    return 0;
}
