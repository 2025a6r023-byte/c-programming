#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    int N, key, position = -1;
    scanf("%d", &N);

    int arr[100];
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    scanf("%d", &key);
    for (int i = 0; i < N; i++) {
        if (arr[i] == key) {
            position = i + 1;  
            break;
        }
    }
    if (position != -1) {
        printf("Element found at position %d\n", position);
    } else {
        printf("Element not found\n");
    }

    return 0;
}
