#include<stdio.h>
#include<math.h>

int main(){
    float x1,y1,x2,y2,r;
    printf("Enter the x and y coordinate of point 1 : ");
    scanf("%f%f",&x1,&y1);
    printf("Enter the x and y coordinate of point 2 : ");
    scanf("%f%f",&x2,&y2);
    r=sqrt(pow(x1-x2,2)+pow(y1-y2,2));
    printf("Euclidean distance between the points is : %f",r);
    return 0;
}
