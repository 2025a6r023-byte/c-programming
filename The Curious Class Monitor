#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


struct Student {
    char name[51];
    int age;
    char grade;
};

int main() {
    struct Student s;

    fgets(s.name, sizeof(s.name), stdin);

    for (int i = 0; s.name[i] != '\0'; i++) {
        if (s.name[i] == '\n') {
            s.name[i] = '\0';
            break;
        }
    }

    scanf("%d", &s.age);
    scanf(" %c", &s.grade); 

    printf("Name: %s\n", s.name);
    printf("Age: %d\n", s.age);
    printf("Grade: %c", s.grade);

    return 0;
}
