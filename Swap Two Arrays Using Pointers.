#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {
    int N;
    scanf("%d", &N);

    int A[100], B[100];

    for (int i = 0; i < N; i++) {
        scanf("%d", (A + i));
    }

    for (int i = 0; i < N; i++) {
        scanf("%d", (B + i));
    }

    for (int i = 0; i < N; i++) {
        int temp = *(A + i);
        *(A + i) = *(B + i);
        *(B + i) = temp;
    }

    for (int i = 0; i < N; i++) {
        printf("%d", *(A + i));
        if (i < N - 1)
            printf(" ");
    }
    printf("\n");

    for (int i = 0; i < N; i++) {
        printf("%d", *(B + i));
        if (i < N - 1)
            printf(" ");
    }

    return 0;
}
